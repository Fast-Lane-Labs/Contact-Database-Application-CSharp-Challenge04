<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <title>Contact App | Fast Lane</title>
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
  
      </head>
      <body>
        <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
          <div class="container">

            <img src="https://labs.fastlane-israel.com/static/assets/img/brand/flane-white.png" alt="Fast Lane Logo" width="166" height="30">
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
              <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarSupportedContent">
              <ul class="navbar-nav me-auto mt-2 mb-lg-0 ms-3">
                <li class="nav-item">
                  <a class="nav-link active" aria-current="page" href="/">Home</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" aria-current="page" href="https://flane.co.il" target="_blank">More Courses</a>
                  </li>
              </ul>
              <!--Login Bar-->
            </div>
          </div>
        </nav>
              <div class="container my-5">
                <nav aria-label="breadcrumb">
                  <ol class="breadcrumb">
                    <li class="breadcrumb-item active" aria-current="page">Home</li>
                    <li class="breadcrumb-item">@ViewContext.RouteData.Values["controller"]</li>
                    <li class="breadcrumb-item">@ViewContext.RouteData.Values["action"]</li>
                  </ol>
                </nav>
                <hr class="col-3 my-4">
         

            @RenderBody()

                      </div>
        </div>

        <div class="container">
            <footer class="d-flex flex-wrap justify-content-between align-items-center py-3 my-4 border-top">
              <div class="col-md-4 d-flex align-items-center">
                <a href="/" class="mb-3 me-2 mb-md-0 text-body-secondary text-decoration-none lh-1">

                  <img src="https://labs.fastlane-israel.com/static/assets/img/brand/favicon.ico" href="https://fastlane.net" alt="Fast Lane Logo" >
                </a>
                <span class="mb-3 mb-md-0 text-body-secondary">© 2024 Fast Lane Israel. All rights reserved. </span>
              </div>
        
            </footer>
          </div>
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>
    @await RenderSectionAsync("Scripts", required: false)
      </body>
    </html>
